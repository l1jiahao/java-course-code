<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" visible="true" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="argon.code.ch06.atm.controller.Login">
   <children>
      <TextField fx:id="loginID" layoutX="240.0" layoutY="126.0" prefHeight="30.0" prefWidth="180.0" promptText="请输入你的账号" />
      <PasswordField fx:id="loginPassword" layoutX="240.0" layoutY="199.0" prefHeight="30.0" prefWidth="180.0" promptText="请输入你的密码" />
      <Label layoutX="179.0" layoutY="130.0" text="账号">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Label layoutX="179.0" layoutY="203.0" text="密码">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Label layoutX="245.0" layoutY="32.0" text="ATM登录">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <Button fx:id="loginBtn" layoutX="231.0" layoutY="273.0" mnemonicParsing="false" onAction="#loginIn" prefHeight="38.0" prefWidth="51.0" text="登录" />
      <Button fx:id="registerBtn" layoutX="351.0" layoutY="273.0" mnemonicParsing="false" onAction="#register" prefHeight="38.0" prefWidth="51.0" text="注册" />
      <Text fx:id="forgetPassword" onMouseClicked="#forget" layoutX="446.0" layoutY="219.0" strokeType="OUTSIDE" strokeWidth="0.0" text="忘记密码" />
   </children>
</AnchorPane>
