<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="argon.code.ch06.atm.controller.ATMSystem">
   <children>
      <Button fx:id="depositBtn" layoutX="93.0" layoutY="90.0" mnemonicParsing="false" onAction="#deposit" text="存款" />
      <Button fx:id="drawBtn" layoutX="93.0" layoutY="178.0" mnemonicParsing="false" onAction="#draw" text="提现" />
      <Button fx:id="transferBtn" layoutX="93.0" layoutY="267.0" mnemonicParsing="false" onAction="#transfer" text="转帐" />
      <Button fx:id="alertBtn" layoutX="461.0" layoutY="90.0" mnemonicParsing="false" onAction="#alert" text="修改信息" />
      <Label layoutX="245.0" layoutY="33.0" text="ATM系统">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button fx:id="tradeBtn" layoutX="461.0" layoutY="179.0" mnemonicParsing="false" onAction="#tradeHistory" text="交易记录" />
      <Label fx:id="outPutInfo" alignment="TOP_LEFT" layoutX="172.0" layoutY="91.0" prefHeight="217.0" prefWidth="255.0" textAlignment="JUSTIFY">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Button fx:id="exitBtn" layoutX="473.0" layoutY="267.0" mnemonicParsing="false" onAction="#exit" text="退出" />
   </children>
</AnchorPane>
